{
  "name": "argser",
  "version": "1.1.16",
  "license": "Unlicense",
  "author": "Chris Ackerman",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shakeskeyboarde/argser.git"
  },
  "keywords": [
    "arg",
    "args",
    "argv",
    "arguments",
    "parse",
    "parser",
    "cli",
    "flag",
    "option",
    "minimist",
    "yargs",
    "commander"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "del-cli lib dist coverage",
    "test": "del-cli coverage && tsc && eslint src && jest --passWithNoTests",
    "build": "del-cli lib && tsc --noEmit false --emitDeclarationOnly && babel src --out-dir lib --extensions .ts,.tsx",
    "prepack": "npm test && npm run build"
  },
  "config": {},
  "browserslist": [
    "node >= 12"
  ],
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.4",
    "@types/prettier": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
